<?xml version="1.0" encoding="utf-8"?>
<wuhj.com.mylibrary.PlaceHoderHeaderLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:id="@+id/fiyw_placehoder">
    <wuhj.com.mylibrary.NotifyingListenerScrollView
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:padding="14dp"
        android:scrollbars="none"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ListView
                android:id="@+id/fiyw_lstv"
                android:scrollbars="none"
                android:listSelector="#00000000"
                android:cacheColorHint="#00000000"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
            <TextView
                android:id="@+id/fiyw_tv_noData"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:visibility="gone"
                android:text="暂时还没发布过作品..."/>
        </LinearLayout>
    </wuhj.com.mylibrary.NotifyingListenerScrollView>
</wuhj.com.mylibrary.PlaceHoderHeaderLayout>