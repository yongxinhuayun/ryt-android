<?xml version="1.0" encoding="utf-8"?>
<wuhj.com.mylibrary.PlaceHoderHeaderLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/v_placehoder"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <wuhj.com.mylibrary.NotifyingListenerScrollView
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:scrollbars="none"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center">
                <ImageView
                    android:layout_marginTop="20dp"
                    android:layout_width="256dp"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/touzimingxingbang"/>
                <LinearLayout
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="21dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="81dp"
                            android:paddingTop="16dp">
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon1"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="58dp"
                                android:layout_height="58dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="68dp"
                                android:layout_height="66dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@mipmap/yin"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title1"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="7dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="#000000"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="14dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:text="投资了"
                            android:textColor="#000000"/>
                        <TextView
                            android:id="@+id/flr_tv_money1"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="¥ 150"
                            android:textColor="#e83f55"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon2"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="74dp"
                                android:layout_height="74dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="78dp"
                                android:layout_height="82dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@mipmap/jin"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title2"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="7dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="#000000"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="14dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:text="投资了"
                            android:textColor="#000000"/>
                        <TextView
                            android:id="@+id/flr_tv_money2"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="¥ 150"
                            android:textColor="#e83f55"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="22dp">
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon3"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="52dp"
                                android:layout_height="52dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="62dp"
                                android:layout_height="60dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@mipmap/tong"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title3"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="7dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="#000000"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="14dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="14sp"
                            android:text="投资了"
                            android:textColor="#000000"/>
                        <TextView
                            android:id="@+id/flr_tv_money3"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="¥ 150"
                            android:textColor="#e83f55"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E9E9E9"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="14dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="32dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="14dp">
                <ImageView
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/zuixintouzijilu"
                    />
                <com.yxh.ryt.custemview.CustomListview
                    android:id="@+id/flr_scroll"
                    android:scrollbars="none"
                    android:layout_marginTop="33dp"
                    android:listSelector="#00000000"
                    android:cacheColorHint="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
        </LinearLayout>
    </wuhj.com.mylibrary.NotifyingListenerScrollView>
</wuhj.com.mylibrary.PlaceHoderHeaderLayout>
