<?xml version="1.0" encoding="utf-8"?>
<wuhj.com.mylibrary.PlaceHoderHeaderLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/v_placehoder"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <wuhj.com.mylibrary.NotifyingListenerScrollView
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:scrollbars="none"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <ImageView
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/touzimingxingbang"/>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon1"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="115dp"
                                android:layout_height="100dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@mipmap/ic_launcher"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title1"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_date1"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="16:00"
                            />
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="投资了"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_money1"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:text="¥ 150"
                            android:textColor="@android:color/holo_red_dark"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon2"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="115dp"
                                android:layout_height="100dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@mipmap/ic_launcher"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title2"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_date2"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="16:00"
                            />
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="投资了"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_money2"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:text="¥ 150"
                            android:textColor="@android:color/holo_red_dark"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                            <com.yxh.ryt.custemview.CircleImageView
                                android:id="@+id/flr_iv_icon3"
                                android:layout_gravity="center_horizontal"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:src="@mipmap/ic_launcher"/>
                            <LinearLayout
                                android:layout_gravity="center_horizontal"
                                android:layout_width="115dp"
                                android:layout_height="100dp"
                                android:gravity="right">
                                <com.yxh.ryt.custemview.CircleImageView
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:src="@mipmap/ic_launcher"
                                    android:layout_gravity="bottom"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/flr_tv_title3"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="喵星人"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_date3"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="16:00"
                            />
                        <TextView
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="投资了"
                            android:textColor="@android:color/black"/>
                        <TextView
                            android:id="@+id/flr_tv_money3"
                            android:gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:text="¥ 150"
                            android:textColor="@android:color/holo_red_dark"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E9E9E9"
                android:layout_margin="15dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/zuixintouzijilu"/>
                <ListView
                    android:id="@+id/flr_scroll"
                    android:scrollbars="none"
                    android:listSelector="#00000000"
                    android:cacheColorHint="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
        </LinearLayout>
    </wuhj.com.mylibrary.NotifyingListenerScrollView>
</wuhj.com.mylibrary.PlaceHoderHeaderLayout>
